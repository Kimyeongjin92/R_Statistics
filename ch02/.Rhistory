##  4-3. ggplot 히스토그램 밀도표
#https://drsimonj.svbtle.com/pretty-histograms-with-ggplot2
ggplot(data,aes(x=V2,fill=cut(..x..,9)))+                      # fill= cut(..변수없을때 기준(count,x)..,막대 개수)
geom_histogram(aes(y = ..density..),
show.legend = F,                        # 범례 숨기기
breaks=seq(0,90,10),closed='left',      # 칸 나누기
col='black',
size=1)+
geom_density(fill="red",alpha=0.2) +
scale_x_continuous(breaks=seq(0,90,10)) +              # 숫자 나누는 기준
scale_fill_discrete(h = c(180, 360), c = 150, l = 80) +
theme_bw(base_family="baedal",base_size=20) +
labs(title='연령별 분포 히스토그램(밀도)',x='연령',y='밀도') +
theme(plot.title = element_text(family="baedal",
face = "bold",
hjust = 0.5,
size = 40,
color = "black"))
##  4-2. ggplot 히스토그램 빈도표
#https://drsimonj.svbtle.com/pretty-histograms-with-ggplot2
ggplot(data,aes(x=V2,fill=cut(..x..,9)))+                      # fill= cut(..변수없을때 기준(count,x)..,막대 개수)
geom_histogram(show.legend = F,                        # 범례 숨기기
breaks=seq(0,90,10),closed='left',      # 칸 나누기
col='black',
size=1)+
scale_x_continuous(breaks=seq(0,90,10)) +              # 숫자 나누는 기준
stat_bin(breaks=seq(0,90,10),                          # 막대 그래프위 숫자.
aes(label=..count..),
closed='left',
vjust=-0.5,
geom = "text",
family="baedal",
size=10) +
scale_fill_discrete(h = c(180, 360), c = 150, l = 80) +
theme_bw(base_family="baedal",base_size=20) +
labs(title='연령별 분포 히스토그램(빈도)',x='연령',y='빈도') +
theme(plot.title = element_text(family="baedal",
face = "bold",
hjust = 0.5,
size = 40,
color = "black"))
